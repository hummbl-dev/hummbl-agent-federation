{
  "version": "0.1.0",
  "default": {
    "network": "none",
    "maxRequestBytes": 262144,
    "maxResponseBytes": 1048576,
    "rateLimitPerMinute": 60
  },
  "allowlist": {
    "domains": [
      "api.openai.com",
      "api.anthropic.com",
      "discord.com",
      "api.twilio.com",
      "gateway.openclaw.ai",
      "hooks.slack.com",
      "api.telegram.org"
    ]
  },
  "denylist": {
    "domains": []
  },
  "notes": "Add vendor domains explicitly before enabling networked skills."
}
